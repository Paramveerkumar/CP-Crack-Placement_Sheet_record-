class Solution {
public:
    int majorityElement(vector<int>& nums) {
      int count=0;
      int majority=0;
      int n=nums.size();
      for(int i=0;i<n;i++)
      {
          if(n==0)
           count=nums[i];
          if(majority=nums[i])
              count++;
          else
              count--;
              
      }
        return majority;
    }
};
